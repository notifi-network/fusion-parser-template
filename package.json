{
  "name": "notifi-fusion-parser",
  "version": "1.0.0",
  "description": "Notifi Fusion Parser",
  "main": "parser.js",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles --forceExit",
    "upload": "tsc && npx rollup -c rollup.config.js && npx fusion-utils parser upload",
    "activate": "npx fusion-utils activate ${npm_config_cursor:+--cursor} ${npm_config_cursor:-}",
    "deactivate": "npx fusion-utils deactivate",
    "auth": "npx fusion-utils auth",
    "init-parser": "npx fusion-utils init parser",
    "build": "tsc && npx rollup -c rollup.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@notifi-network/dev-utils": "^2.0.10",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "30.0.0",
    "@types/node": "22.12.0",
    "jest": "^30.0.5",
    "rollup": "^3.21.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@notifi-network/fusion-sdk": "0.1.0-canary.2",
    "viem": "1.21.1",
    "zod": "3.22.4"
  }
}
